{
    "name": "@buildingai/upload",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "check-types": "vue-tsc --noEmit",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch"
    },
    "type": "module",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "main": "./src/index.ts",
    "files": [
        "src"
    ],
    "exports": {
        ".": "./src/index.ts"
    },
    "dependencies": {
        "@buildingai/constants": "workspace:*",
        "@buildingai/service": "workspace:*"
    }
}
